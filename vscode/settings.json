{
  "editor.cursorSmoothCaretAnimation": "on",
  "explorer.autoReveal": true,
  "workbench.colorCustomizations": {
    "activityBar.background": "#000000",
    "editor.background": "#000000",
    "sideBar.background": "#000000",
    "sideBar.border": "#000000",
    "titleBar.activeBackground": "#000000",
    "titleBar.inactiveBackground": "#000000",
    "titleBar.border": "#000000",
    "statusBar.noFolderBackground": "#000001",
    "statusBar.noFolderBorder": "#000000",
    "statusBar.background": "#000000",
    "statusBar.border": "#000000",
    "terminal.background": "#000000",
    "editorGroupHeader.tabsBackground": "#000000",
    "tab.activeBackground": "#000000",
    "tab.inactiveBackground": "#000000",
    "tab.border": "#000000",
    "breadcrumb.background": "#000000",
    "editorGroupHeader.background": "#000000",
    "panel.background": "#000000",
    "editorGroup.border": "#000000",
    "editorGutter.background": "#000000",
    "welcomePage.background": "#000000",
    "editor.emptySelectionBackground": "#000000"
  },
  "files.autoSave": "afterDelay",
  "git.autofetch": true,
  "editor.fontFamily": "Cascadia Code NF",
  "editor.fontSize": 15,
  "editor.lineHeight": 1.7,
  "editor.renderLineHighlight": "gutter",
  "editor.fontLigatures": true,
  "explorer.compactFolders": false,
  "editor.semanticHighlighting.enabled": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "workbench.settings.applyToAllProfiles": [
    "editor.fontFamily"
  ],
  "terminal.integrated.fontFamily": "Cascadia Code NF",
  "apc.electron": {
    "titleBarStyle": "hiddenInset"
  },
  "apc.stylesheet": {
    ".title-label > h2": "display: none",
    ".window-title": "display: none",
    ".decorationsOverviewRuler": "max-width : 0px"
  },
  "apc.font.family": "Cascadia Code NF",
  "window.titleBarStyle": "native",
  "symbols.hidesExplorerArrows": false,
  "window.menuBarVisibility": "toggle",
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": false,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.enabled": false,
  "window.customTitleBarVisibility": "auto",
  "breadcrumbs.enabled": false,
  "workbench.editor.showTabs": "none",
  "editor.inlineSuggest.enabled": true,
  "emmet.triggerExpansionOnTab": true,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "git.openRepositoryInParentFolders": "always",
  "workbench.activityBar.location": "hidden",
  "go.toolsManagement.autoUpdate": true,
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "workbench.iconTheme": "catppuccin-macchiato",
  "cmake.options.statusBarVisibility": "visible",
  "telemetry.telemetryLevel": "off",
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "editor.experimentalInlineEdit.enabled": true,
  "editor.inlineCompletionsAccessibilityVerbose": true,
  "editor.inlineSuggest.suppressSuggestions": true,
  "debug.allowBreakpointsEverywhere": true,
  "editor.rulers": [
    120
  ],
  "separators.classes.borderStyle": "dashed",
  "separators.constructors.borderStyle": "dashed",
  "separators.enums.borderStyle": "dashed",
  "separators.functions.borderStyle": "dashed",
  "separators.interfaces.borderStyle": "dashed",
  "separators.methods.borderStyle": "dashed",
  "separators.namespaces.borderStyle": "dashed",
  "separators.structs.borderStyle": "dashed",
  "workbench.colorTheme": "Catppuccin Macchiato",
  "svelte.enable-ts-plugin": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "github.copilot.chat.followUps": "never",
  "github.copilot.chat.terminalChatLocation": "terminal",
  "explorer.confirmDelete": false,
}